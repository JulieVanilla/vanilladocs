    <!-- bower:css -->
    <!-- endbower -->
    <!--
    All custom stylesheets should be included here. They will be concatenated
    with the dependencies installed via Bower when building the docs.
    -->
    <link rel="stylesheet" href="/assets/docs.css">
    <!-- endbuild -->
  </head>
  <body>
